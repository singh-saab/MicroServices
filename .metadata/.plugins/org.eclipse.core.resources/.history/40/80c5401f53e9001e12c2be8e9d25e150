package com.MS.entities;

import java.time.LocalTime;

public class User {
	
	int id;
	String name;
	LocalTime birthdate;
	
	public User() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public User(int id ,String name, LocalTime birthdate) {
		super();
		this.id = id;
		this.name = name;
		this.birthdate = birthdate;
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public LocalTime getBirthdate() {
		return birthdate;
	}
	public void setBirthdate(LocalTime birthdate) {
		this.birthdate = birthdate;
	}
}
